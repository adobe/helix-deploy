{
  "name": "@adobe/helix-deploy",
  "version": "5.1.0",
  "description": "Library and Commandline Tools to build and deploy OpenWhisk Actions",
  "license": "Apache-2.0",
  "homepage": "https://github.com/adobe/helix-deploy#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-deploy.git"
  },
  "bugs": {
    "url": "https://github.com/adobe/helix-deploy/issues"
  },
  "keywords": [
    "helix",
    "serverless"
  ],
  "main": "index.js",
  "type": "module",
  "bin": {
    "hedy": "src/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "c8 mocha -i -g Integration",
    "test-ci": "npm run lint && c8 mocha -i -g Integration && codecov",
    "integration-ci": "c8 mocha -g Integration && codecov",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "mocha": {
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json",
    "require": "test/setup-env.js"
  },
  "peerDependencies": {
    "@adobe/helix-universal": ">=1.9.0"
  },
  "dependencies": {
    "@adobe/fastly-native-promises": "2.0.0",
    "@adobe/helix-fetch": "3.0.0",
    "@aws-sdk/client-apigatewayv2": "3.46.0",
    "@aws-sdk/client-lambda": "3.46.0",
    "@aws-sdk/client-s3": "3.46.0",
    "@aws-sdk/client-secrets-manager": "3.46.0",
    "@aws-sdk/client-ssm": "3.46.0",
    "@azure/arm-appservice": "9.0.0",
    "@azure/ms-rest-nodeauth": "3.1.1",
    "@fastly/js-compute": "0.2.1",
    "@google-cloud/functions": "1.2.0",
    "@google-cloud/secret-manager": "3.10.1",
    "@google-cloud/storage": "5.17.0",
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "archiver": "5.3.0",
    "chalk-template": "0.2.0",
    "constants-browserify": "1.0.0",
    "dotenv": "14.1.0",
    "express": "4.17.2",
    "form-data": "4.0.0",
    "fs-extra": "10.0.0",
    "get-stream": "6.0.1",
    "isomorphic-git": "1.10.3",
    "openwhisk": "3.21.6",
    "proxyquire": "2.1.3",
    "rollup": "2.63.0",
    "rollup-plugin-terser": "7.0.2",
    "semver": "7.3.5",
    "tar": "6.1.11",
    "webpack": "5.65.0",
    "yargs": "17.3.1"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.3.2",
    "@adobe/helix-shared-wrap": "1.0.0",
    "@adobe/helix-status": "9.2.4",
    "@adobe/helix-universal-logger": "2.0.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "c8": "7.11.0",
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "codecov": "3.8.3",
    "eslint": "8.6.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.25.4",
    "husky": "7.0.4",
    "lint-staged": "12.1.7",
    "mocha": "9.1.3",
    "mocha-junit-reporter": "2.0.2",
    "mocha-multi-reporters": "1.5.1",
    "nock": "13.2.2",
    "semantic-release": "18.0.1",
    "sinon": "12.0.1",
    "yauzl": "2.10.0"
