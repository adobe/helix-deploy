{
  "name": "@adobe/helix-deploy",
  "version": "4.11.5",
  "description": "Library and Commandline Tools to build and deploy OpenWhisk Actions",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-deploy.git"
  },
  "bugs": {
    "url": "https://github.com/adobe/helix-deploy/issues"
  },
  "keywords": [
    "openwhisk"
  ],
  "main": "index.js",
  "bin": {
    "hedy": "src/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "nyc mocha -i -g Integration",
    "test-ci": "npm run lint && nyc mocha -i -g Integration && codecov",
    "integration-ci": "nyc mocha --reporter-options=configFile=.mocha-multi.json -g Integration && codecov",
    "semantic-release": "semantic-release"
  },
  "mocha": {
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json",
    "require": "test/setup-env.js"
  },
  "peerDependencies": {
    "@adobe/helix-universal": ">=1.6.0"
  },
  "dependencies": {
    "@adobe/fastly-native-promises": "1.24.0",
    "@adobe/helix-fetch": "3.0.0",
    "@aws-sdk/client-apigatewayv2": "3.38.0",
    "@aws-sdk/client-lambda": "3.38.0",
    "@aws-sdk/client-s3": "3.38.0",
    "@aws-sdk/client-secrets-manager": "3.38.0",
    "@aws-sdk/client-ssm": "3.38.0",
    "@azure/arm-appservice": "9.0.0",
    "@azure/ms-rest-nodeauth": "3.1.0",
    "@google-cloud/functions": "1.2.0",
    "@google-cloud/secret-manager": "3.10.1",
    "@google-cloud/storage": "5.15.3",
    "archiver": "5.3.0",
    "chalk": "4.1.2",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "fs-extra": "10.0.0",
    "isomorphic-git": "1.10.1",
    "openwhisk": "3.21.4",
    "proxyquire": "2.1.3",
    "semver": "7.3.5",
    "webpack": "5.59.1",
    "yargs": "17.2.1"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.3.0",
    "@adobe/helix-shared-wrap": "1.0.0",
    "@adobe/helix-status": "9.2.4",
    "@adobe/helix-universal-logger": "2.0.0",
    "@semantic-release/changelog": "6.0.0",
    "@semantic-release/git": "10.0.0",
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "codecov": "3.8.3",
    "eslint": "8.1.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.25.2",
    "ghooks": "2.0.4",
    "lint-staged": "11.2.4",
    "mocha": "9.1.3",
    "mocha-junit-reporter": "2.0.2",
    "mocha-multi-reporters": "1.5.1",
    "nock": "13.1.4",
    "nyc": "15.1.0",
    "semantic-release": "18.0.0",
    "sinon": "11.1.2",
    "yauzl": "2.10.0"
  },
  "engines": {
    "node": ">=12.0"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "homepage": "https://github.com/adobe/helix-deploy#readme"
}
