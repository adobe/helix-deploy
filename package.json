{
  "name": "@adobe/helix-deploy",
  "version": "11.0.2",
  "description": "Library and Commandline Tools to build and deploy OpenWhisk Actions",
  "license": "Apache-2.0",
  "homepage": "https://github.com/adobe/helix-deploy#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-deploy.git"
  },
  "bugs": {
    "url": "https://github.com/adobe/helix-deploy/issues"
  },
  "keywords": [
    "helix",
    "serverless"
  ],
  "main": "index.js",
  "type": "module",
  "bin": {
    "hedy": "src/index.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "c8 mocha -i -g Integration",
    "integration-ci": "c8 mocha -g Integration",
    "semantic-release": "semantic-release",
    "semantic-release-dry": "semantic-release --dry-run --branches $CI_BRANCH",
    "prepare": "husky install"
  },
  "mocha": {
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json",
    "require": "test/setup-env.js"
  },
  "peerDependencies": {
    "@adobe/helix-universal": ">=4.1.1"
  },
  "dependencies": {
    "@adobe/fastly-native-promises": "3.0.6",
    "@adobe/fetch": "4.1.1",
    "@adobe/helix-shared-process-queue": "3.0.1",
    "@aws-sdk/client-apigatewayv2": "3.504.0",
    "@aws-sdk/client-lambda": "3.504.0",
    "@aws-sdk/client-s3": "3.504.0",
    "@aws-sdk/client-secrets-manager": "3.504.0",
    "@aws-sdk/client-ssm": "3.504.0",
    "@aws-sdk/client-sts": "3.504.0",
    "@fastly/js-compute": "3.8.2",
    "@google-cloud/functions": "3.0.1",
    "@google-cloud/secret-manager": "5.0.1",
    "@google-cloud/storage": "7.7.0",
    "@rollup/plugin-alias": "5.1.0",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "archiver": "6.0.1",
    "chalk-template": "1.1.0",
    "constants-browserify": "1.0.0",
    "dotenv": "16.4.1",
    "form-data": "4.0.0",
    "fs-extra": "11.2.0",
    "isomorphic-git": "1.25.3",
    "openwhisk": "3.21.8",
    "semver": "7.5.4",
    "tar": "6.2.0",
    "webpack": "5.90.1",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "2.0.6",
    "@adobe/helix-shared-wrap": "2.0.1",
    "@adobe/helix-status": "10.0.11",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "c8": "9.1.0",
    "eslint": "8.56.0",
    "husky": "9.0.10",
    "lint-staged": "15.2.1",
    "mocha": "10.2.0",
    "mocha-junit-reporter": "2.2.1",
    "mocha-multi-reporters": "1.5.1",
    "nock": "13.5.1",
    "semantic-release": "22.0.12",
    "yauzl": "2.10.0"
  },
  "engines": {
    "node": ">=12.0"
  },
  "lint-staged": {
    "*.js": "eslint"
  }
}
